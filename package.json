{
  "name": "span-promise",
  "engines": {
    "node": ">=8.9.1"
  },
  "version": "1.0.0",
  "description": "js promiced enhanced with time conditions",
  "main": "bind/index.js",
  "commentedScripts": {
    "watch": "webpack --progress --colors --watch --config ./webpack.config.js"
  },
  "scripts": {
    "build": "babel-node ./scripts/emptyFolder && webpack --config ./webpack.prod.config.js",
    "dev": "webpack-dev-server --content-base src/index --hot --host 0.0.0.0 --config ./webpack.config.js",
    "test": "karma start --reporters html",
    "compile": "rm -rf .compiled && babel src --out-dir .compiled/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tymiurge/span-promise.git"
  },
  "keywords": [
    "js",
    "promice",
    "time"
  ],
  "author": "tymiurge",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "es6-promise": "^4.1.1",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^11.2.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-htmlfile-reporter": "~0.3",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/tymiurge/span-promise/issues"
  },
  "homepage": "https://github.com/tymiurge/span-promise#readme"
}
